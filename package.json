{
  "name": "pedeffy",
  "version": "0.1.0",
  "description": "Generate PDF files from a server and serve these as downloads",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run dist && node dist/index.js",
    "dist": "babel src --out-dir dist --presets es2015,react,babel-polyfill --plugins syntax-decorators",
    "example": "babel . --ignore node_modules,dist --out-dir example-dist --presets es2015,react,babel-polyfill --plugins syntax-decorators && node example-dist/index.js",
    "watch": "babel src --out-dir dist -w --presets es2015,react,babel-polyfill --plugins syntax-decorators",
    "prepublish": "npm run dist",
    "lint": "eslint ."
  },
  "author": "inventid <info@inventid.nl>",
  "license": "MIT",
  "dependencies": {
    "@react-pdf/core": "^0.4.6",
    "@react-pdf/dom": "^0.4.6",
    "@react-pdf/node": "^0.4.6",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "fs-extra": "^4.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "uuid-v4": "^0.1.0"
  },
  "devDependencies": {
    "babel-cli": "~6.22.2",
    "babel-core": "~6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-runtime": "~6.15.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-react": "^6.22.0",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^6.9.0",
    "prop-types": "^15.6.0"
  }
}
